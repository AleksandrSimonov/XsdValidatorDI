<?xml version="1.0" encoding="UTF-8"?>
<xs:schema xmlns="http://ip.cbr.ru/objects" xmlns:xs="http://www.w3.org/2001/XMLSchema" targetNamespace="http://ip.cbr.ru/objects" elementFormDefault="qualified" attributeFormDefault="unqualified">
	<!--***** COMPLEX TYPES *****-->
	<xs:complexType name="AccountingPeriodType">
		<xs:annotation>
			<xs:documentation>Отчетный период (налогообложение)</xs:documentation>
		</xs:annotation>
		<xs:sequence>
			<xs:element name="year" type="TaxYearType">
				<xs:annotation>
					<xs:documentation>Отчетный год (начиная с 2017)</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="quarter" type="QuarterType">
				<xs:annotation>
					<xs:documentation>Номер отчетного квартала. Допустимые значения: 1-4. Указывается последний квартал отчетного (налогового) периода (поле «Отчетный период» документа «Расчет налога»): для отчета за 1-й квартал указывается 1, за полугодие - 2, за 9 месяцев - 3,  за год - 4</xs:documentation>
				</xs:annotation>
			</xs:element>
		</xs:sequence>
	</xs:complexType>
	<xs:complexType name="AdditionalPropertiesType">
		<xs:annotation>
			<xs:documentation>Блок дополнительных атрибутов (используется для расширения состава передаваемых данных без изменения версии сообщения или интерфейса)</xs:documentation>
		</xs:annotation>
		<xs:sequence>
			<xs:element name="property" type="PropertyType" maxOccurs="unbounded">
				<xs:annotation>
					<xs:documentation>Атрибут</xs:documentation>
				</xs:annotation>
			</xs:element>
		</xs:sequence>
	</xs:complexType>
	<xs:complexType name="AdvanceType">
		<xs:annotation>
			<xs:documentation>Аванс</xs:documentation>
		</xs:annotation>
		<xs:sequence>
			<xs:element name="amountRURwVAT" type="MoneyType" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Сумма в рублях (с учетом НДС)</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="amountRURwoVAT" type="MoneyType" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Сумма в рублях (без НДС)</xs:documentation>
				</xs:annotation>
			</xs:element>
		</xs:sequence>
	</xs:complexType>
	<xs:complexType name="AssignmentsListType">
		<xs:annotation>
			<xs:documentation>Список отнесений</xs:documentation>
		</xs:annotation>
		<xs:sequence>
			<xs:element name="assignment" type="AssignmentType" maxOccurs="unbounded">
				<xs:annotation>
					<xs:documentation>Данные отнесения</xs:documentation>
				</xs:annotation>
			</xs:element>
		</xs:sequence>
	</xs:complexType>
	<xs:complexType name="AssignmentType">
		<xs:annotation>
			<xs:documentation>Данные отнесения</xs:documentation>
		</xs:annotation>
		<xs:sequence>
			<xs:element name="date" type="DateType">
				<xs:annotation>
					<xs:documentation>Дата операции</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="amount" type="PriceType">
				<xs:annotation>
					<xs:documentation>Сумма отнесения</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="expenses" type="ExpenseRef" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Статья затрат</xs:documentation>
				</xs:annotation>
			</xs:element>
		</xs:sequence>
	</xs:complexType>
	<xs:complexType name="AsfpContractInfoType">
		<xs:annotation>
			<xs:documentation>Данные договора</xs:documentation>
		</xs:annotation>
		<xs:sequence>
			<xs:element name="ContractGUID" type="String36">
				<xs:annotation>
					<xs:documentation>Идентификатор договора. Уникальный 
идентификатор 
договора по данным 
ЕИС СЗ</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="ContractNumber" type="String25">
				<xs:annotation>
					<xs:documentation>Номер договора</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="ContractDate" type="DateType">
				<xs:annotation>
					<xs:documentation>Дата заключения договора</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="ContractNumberERD" type="String50">
				<xs:annotation>
					<xs:documentation>Номер договора ЕРД. Указывается реестровый номер договора</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="ContractValue" type="Float17_2">
				<xs:annotation>
					<xs:documentation>Сумма договора (для операций по договорам равна сумме договора, для бездоговорных операций равна сумме документа)
</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="ContractPaymentsList" type="AsfpContractPaymentsListType" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Оплата по договору / ДС</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="ContractAssignmentsList" type="ContractAssignmentsListType" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Список отнесений на смету</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="reversalList" type="reversalListType" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Список операций сторно</xs:documentation>
				</xs:annotation>
			</xs:element>
		</xs:sequence>
	</xs:complexType>
	<xs:complexType name="AsfpContractPaymentsListType">
		<xs:annotation>
			<xs:documentation>Оплата по договору / ДС</xs:documentation>
		</xs:annotation>
		<xs:sequence>
			<xs:element name="ContractPayments" type="AsfpContractPaymentsType" maxOccurs="unbounded">
				<xs:annotation>
					<xs:documentation>Информация о платеже</xs:documentation>
				</xs:annotation>
			</xs:element>
		</xs:sequence>
	</xs:complexType>
	<xs:complexType name="AsfpContractPaymentsType">
		<xs:annotation>
			<xs:documentation>Информация о платеже</xs:documentation>
		</xs:annotation>
		<xs:sequence>
			<xs:element name="TransactionDate" type="DateType">
				<xs:annotation>
					<xs:documentation>Дата операции</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="documentGUID" type="String36">
				<xs:annotation>
					<xs:documentation>GUID документа операции. Уникальный идентификатор документа операции в АС ВХД</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="documentDate" type="DateType">
				<xs:annotation>
					<xs:documentation>Дата документа об оплате. Дата документа в АС ВХД</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="documentNumber" type="String9">
				<xs:annotation>
					<xs:documentation>Номер документа об оплате. Порядковый номер документа в АС ВХД</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="purpose" type="String1000" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Назначение платежа</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="Amount" type="Float17_2">
				<xs:annotation>
					<xs:documentation>Сумма платежа в рублях</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="CurrencyId" type="String3">
				<xs:annotation>
					<xs:documentation>Валюта, числовой код</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="AmountVal" type="Float17_2">
				<xs:annotation>
					<xs:documentation>Сумма платежа в валюте</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="ExpenseItemGUID" type="String36">
				<xs:annotation>
					<xs:documentation>Статья затрат. Уникальный идентификатор статьи сметы</xs:documentation>
				</xs:annotation>
			</xs:element>
		</xs:sequence>
	</xs:complexType>
	<xs:complexType name="AttachmentsType">
		<xs:annotation>
			<xs:documentation>Данные о вложениях</xs:documentation>
		</xs:annotation>
		<xs:sequence>
			<xs:element name="File" type="FileType" maxOccurs="unbounded">
				<xs:annotation>
					<xs:documentation>Файл-вложение</xs:documentation>
				</xs:annotation>
			</xs:element>
		</xs:sequence>
	</xs:complexType>
	<xs:simpleType name="AttachmentTypeRef">
		<xs:annotation>
			<xs:documentation>Тип вложения</xs:documentation>
		</xs:annotation>
		<xs:restriction base="xs:string">
			<xs:enumeration value="Договор"/>
			<xs:enumeration value="Дополнительное соглашение"/>
			<xs:enumeration value="Счет"/>
			<xs:enumeration value="товарная накладная"/>
			<xs:enumeration value="товарно-транспортная накладная"/>
			<xs:enumeration value="железнодорожная накладная"/>
			<xs:enumeration value="авианакладная"/>
			<xs:enumeration value="коносамент"/>
			<xs:enumeration value="акт о приемке товаров"/>
			<xs:enumeration value="акт о приемке товара, поступившего без счета поставщика"/>
			<xs:enumeration value="акт о приеме-передачи основных средств (кроме зданий, сооружений)"/>
			<xs:enumeration value="акт о приеме-передачи кроме здания (сооружения)"/>
			<xs:enumeration value="акт о приеме-передачи групп объектов основных средств (кроме зданий, сооружений)"/>
			<xs:enumeration value="акт о приеме-передачи отремонтированных, реконструированных, модернизированных объектов основных средств"/>
			<xs:enumeration value="акт о приемке выполненных работ"/>
			<xs:enumeration value="акт приемки законченного строительством объекта"/>
			<xs:enumeration value="счет на оплату"/>
			<xs:enumeration value="Счет-фактура"/>
			<xs:enumeration value="акт выполненных работ"/>
			<xs:enumeration value="акт о приемке материалов"/>
			<xs:enumeration value="акт о завесе тары"/>
			<xs:enumeration value="акт об оприходовании тары, не указанной в счете поставщика"/>
			<xs:enumeration value="акт о выявлении недостатков выполненных работ (оказанных услуг)"/>
			<xs:enumeration value="акт о невыполнении работ (оказании услуг)"/>
			<xs:enumeration value="акт-рекламация"/>
			<xs:enumeration value="Акт о приеме (поступлении) оборудования"/>
			<xs:enumeration value="прочие документы о приемке, не указанные выше"/>
			<xs:enumeration value="Чек"/>
			<xs:enumeration value="Кассовый чек"/>
			<xs:enumeration value="Товарный чек"/>
			<xs:enumeration value="Бланк строгой отчетности"/>
			<xs:enumeration value="Отчет"/>
			<xs:enumeration value="Акт о приеме-сдаче выполненных работ (оказанных услуг)"/>
			<xs:enumeration value="Акт о сдаче-приеме товара"/>
			<xs:enumeration value="Универсальный передаточный документ"/>
			<xs:enumeration value="Акт о предоставлении услуг подразделением общественного питания по проведению мероприятия за счет сметного финансирования"/>
			<xs:enumeration value="Акт о приеме-передаче программных продуктов, прав использования программных продуктов"/>
			<xs:enumeration value="Акт о передаче-приеме имущества в ремонт, на реконструкцию, модернизацию, поверку"/>
			<xs:enumeration value="Акт о приеме-сдаче имущества после проведения ремонта, реконструкции,  модернизации, поверки"/>
			<xs:enumeration value="Акт о сдаче-приеме оказанных образовательных (консультационных) услуг"/>
			<xs:enumeration value="Акт о фактических расходах на проведение культурно-оздоровительного мероприятия"/>
			<xs:enumeration value="Акт о фактических расходах на проведение совещания"/>
			<xs:enumeration value="Акт о фактических расходах на проведение представительского мероприятия"/>
			<xs:enumeration value="Акт о  передаче-приеме оборудования в монтаж"/>
			<xs:enumeration value="Акт о вводе в эксплуатацию  объекта (группы объектов) основных средств, включая здания (сооружения)"/>
			<xs:enumeration value="Акт о приеме-передаче объекта (группы объектов) основных средств, включая здания (сооружения)"/>
			<xs:enumeration value="Иная форма документа зафиксированная договором"/>
			<xs:enumeration value="Допуск к работам до начала заключения договора"/>
		</xs:restriction>
	</xs:simpleType>
	<xs:complexType name="BinaryDataType">
		<xs:annotation>
			<xs:documentation>Контент универсального (общего) сообщения (бинарные данные)</xs:documentation>
		</xs:annotation>
		<xs:sequence>
			<xs:element name="name" type="xs:string" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Наименование передаваемых данных (например, для сохранения полученных данных в файл)</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="dataFormat" type="xs:string">
				<xs:annotation>
					<xs:documentation>Формат (формат данных, представленных в элементе "contents": ‘ZIP’, ‘XML’, ‘PKCS#7’, ‘EDIFACT’ и т.п.)</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="contents" type="xs:base64Binary">
				<xs:annotation>
					<xs:documentation>Контент сообщения в кодировке base64</xs:documentation>
				</xs:annotation>
			</xs:element>
		</xs:sequence>
	</xs:complexType>
	<xs:complexType name="ContractLimitsListType">
		<xs:annotation>
			<xs:documentation>Тим информация об объекте</xs:documentation>
		</xs:annotation>
		<xs:sequence>
			<xs:element name="ContractLimits" type="ContractLimitsType" maxOccurs="unbounded">
				<xs:annotation>
					<xs:documentation>Лимит договора</xs:documentation>
				</xs:annotation>
			</xs:element>
		</xs:sequence>
	</xs:complexType>
	<xs:complexType name="ContractLimitsType">
		<xs:annotation>
			<xs:documentation>Тип лимит контракта</xs:documentation>
		</xs:annotation>
		<xs:sequence>
			<xs:element name="Header" type="HeaderType">
				<xs:annotation>
					<xs:documentation>Шапка лимитов договора</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="CostTable" type="CostTableType">
				<xs:annotation>
					<xs:documentation>Табличная часть «Стоимостные показатели»</xs:documentation>
				</xs:annotation>
			</xs:element>
		</xs:sequence>
	</xs:complexType>
	<xs:complexType name="ContractLimitsMessageType">
		<xs:annotation>
			<xs:documentation>Тип сообщения "Лимиты по договору"</xs:documentation>
		</xs:annotation>
		<xs:sequence>
			<xs:element name="messageInfo" type="MessageInfoType">
				<xs:annotation>
					<xs:documentation>Информация о сообщении</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="ContractLimitsList" type="ContractLimitsListType">
				<xs:annotation>
					<xs:documentation>Список лимитов по договору</xs:documentation>
				</xs:annotation>
			</xs:element>
		</xs:sequence>
	</xs:complexType>
	<xs:complexType name="CommonMessageInfoType">
		<xs:annotation>
			<xs:documentation>Данные универсального (общего) сообщения</xs:documentation>
		</xs:annotation>
		<xs:sequence>
			<xs:element name="purpose" type="xs:string">
				<xs:annotation>
					<xs:documentation>Тип назначения данных (согласованный между отправителем и получателем идентификатор назначения данных. Например, VEHICLE_DIC_DATA – реквизиты ТС; VEHICLE_SCAN_COPY – скан копия документа ТС; VEHICLE_DATA_INQUIRY – запрос на пересылку справочника ТС; DEPRECIATION_OF_REAL_ESTATE - данные по суммам амортизации на объекты недвижимости; VHD_UISPP_DATA - передача данных по участникам ПП из АС ВХД в УИС ПП; NDS – данные по НДС за отчетный квартал сформированные в АС ВХД; NDS3 – данные по НДС с учетом коэффициента пропорции сформированные в АС ВХД; NDSK – уточненные данные по НДС сформированные в АС ВХД; PRFN – данные по налогу на прибыль за отчетный период сформированные в АС ВХД; PRFK – уточненные данные по налогу на прибыль сформированные в АС ВХД; NDSO_RESPONSE – квитанция о доставке формы NDSO сформированная в АС ВХД; PRFO_RESPONSE – квитанция о доставке формы PRFO сформированная в АС ВХД; NDSO – запрос на уточнение данных отчета по НДС сформированный в КЗ НУиНО; PRF2 – декларация по налогу на прибыль сформированная КЗ НУиНО; PFRO – запрос на уточнение данных отчета по налогу на прибыль сформированный в КЗ НУиНО; NDS_RESPONSE – квитанция о доставке/обработке формы NDS сформированная в КЗ НУиНО; NDS3_RESPONSE – квитанция о доставке/обработке формы NDS3 сформированная в КЗ НУиНО; NDSK_RESPONSE – квитанция о доставке/обработке формы NDSK сформированная в КЗ НУиНО; PRFN_RESPONSE – квитанция о доставке/обработке формы PRFN сформированная в КЗ НУиНО; PRFK_RESPONSE квитанция о доставке/обработке формы PRFK сформированная в КЗ НУиНО; UNIS_DEPRECIATION_OF_REAL_ESTATE - суммы начисленной амортизации. </xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="messageObject" type="xs:string" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Идентификатор объекта сообщения (может использоваться, например, для связи нескольких сообщений между собой)</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="binaryData" type="BinaryDataType" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Контент сообщения (бинарные данные)</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="additionalProperties" type="AdditionalPropertiesType" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Дополнительные реквизиты сообщения в формате "ключ-значение"</xs:documentation>
				</xs:annotation>
			</xs:element>
		</xs:sequence>
	</xs:complexType>
	<xs:complexType name="CommonMessageType">
		<xs:annotation>
			<xs:documentation>Универсальное (общее) сообщение</xs:documentation>
		</xs:annotation>
		<xs:sequence>
			<xs:element name="messageInfo" type="MessageInfoType">
				<xs:annotation>
					<xs:documentation>Информация о сообщении</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="commonMessageInfo" type="CommonMessageInfoType">
				<xs:annotation>
					<xs:documentation>Данные универсального (общего) сообщения</xs:documentation>
				</xs:annotation>
			</xs:element>
		</xs:sequence>
	</xs:complexType>
	<xs:complexType name="ContractAssignmentsListType">
		<xs:annotation>
			<xs:documentation>Список отнесений договора на смету</xs:documentation>
		</xs:annotation>
		<xs:sequence>
			<xs:element name="ContractAssignments" type="ContractAssignmentsType" maxOccurs="unbounded">
				<xs:annotation>
					<xs:documentation>Информация об отнесении</xs:documentation>
				</xs:annotation>
			</xs:element>
		</xs:sequence>
	</xs:complexType>
	<xs:complexType name="ContractAssignmentsType">
		<xs:annotation>
			<xs:documentation>Информация об отнесении договора на смету</xs:documentation>
		</xs:annotation>
		<xs:sequence>
			<xs:element name="TransactionDate" type="DateType">
				<xs:annotation>
					<xs:documentation>Дата операции</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="documentGUID" type="String36">
				<xs:annotation>
					<xs:documentation>GUID документа операции. Уникальный идентификатор документа операции в АС ВХД</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="documentDate" type="DateType">
				<xs:annotation>
					<xs:documentation>Дата документа в АС ВХД</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="documentNumber" type="String9">
				<xs:annotation>
					<xs:documentation>Номер документа. Порядковый номер документа в АС ВХД</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="TransactionType" type="String100">
				<xs:annotation>
					<xs:documentation>Вид операции АС ВХД</xs:documentation>
